    <script>
    $('body').on('click', '[data-authorized="0"]', function(e) {
        e.preventDefault();
        $('#loginModalWindow').modal();
    });
    $('body').on('click', '.add-comment__form iframe', function(e) {
        const preview = $(this).closest('.add-comment__form').find('.add-comment__preview');
        const showPreview = $(this).closest('.add-comment__form').find('.add-comment__preview-show');

        showPreview.collapse('show');
        preview.collapse('hide');

    });
    $('body').on('click', '.add-comment__preview-show', function(e) {
        const preview = $(this).closest('.add-comment__form').find('.add-comment__preview');
        const iframe = $(this).closest('.add-comment__form').find('.add-comment__textarea iframe')[0];
        const showPreview = this;

        preview[0].innerHTML = iframe.contentDocument.querySelector('body').innerHTML;
        if (preview.attr('data-shown') != 'true') {
            $(iframe.contentDocument.querySelector('body')).on('mousedown', function(e) {
                preview.collapse('hide');
                $(showPreview).collapse('show');
            })
            preview.attr('data-shown', 'true');
        }
    })

    $('body').on('click', '.brf_submit', function(e) {
        $(this).closest('.add-comment__form').find('.file-fileUploader').click();
    });
    
    $(document).on('dragenter', '.brf_submit' , function(e) {
        if (!$(this).hasClass('brf_submit--dropbox_shown')) {
            $(this).find('ins').click();
            $(this).addClass('brf_submit--dropbox_shown');
        }
    });
    
    BX.addCustomEvent('StopUpload', function(e) {
        setTimeout(() => {
            if (!$(e.controller).closest('.files-preview__send-wrap').find('input[id^="file-doc"]').length) {
                    $(e.controller).closest('.files-preview__send-wrap').find('.file-selector_hidden').removeClass('file-selector_hidden');
                }
        }, 0);
    });
    $('body').on('click', '.add-comment__submit', function(e) {
        if ($(e.target).closest('.add-comment__form').find('.lha-textarea').val().length) {
            e.target.disabled = 'disabled';
        }
    })
    BX.addCustomEvent('onAjaxSuccess', function(response, event) {
        if (response?.request?.request_type == 'voting') {
            window?.realcommenter_admintools_scan_and_init();
        } else if (response?.request?.request_type == 'add') {
            if (response.ERROR) {
                let form = response.request.target_position ? $('[data-form-target="' + response.request.target_position + '"]') : $('[data-form-to="' + response.request.target_node + '"]');
                form.find('.add-comment__submit').removeAttr('disabled');
            } else {
                if (response.request.target_position) {
                    $('[data-zero="' + response.request.target_position + '"]').closest('.brn_add').collapse('show');
                }
            }
        } else if (response?.request?.request_type == 'get') {
            if (!response.ERROR) {
                let form = response.request.comment_target ? $('[data-form-target="' + response.request.comment_target + '"]') : $('[data-form-to="' + response.request.comment_id + '"]');
                $('[data-zero="' + response.request.comment_target + '"]').closest('.brn_add').collapse('hide');
                form.collapse('show');
            }
        }
    });
    BX.addCustomEvent('onAjaxFailure', function(...args) {
        let e = args[2];
        if (e?.url?.includes('realcommenter')) {
            let data = new URLSearchParams(e.data);
            if (data.get('request_type') == 'add') {
                let form = data.get('target_position') ? $('[data-form-target="' + data.get('target_position') + '"]') : $('[data-form-to="' + data.get('target_node') + '"]');
                form.find('.add-comment__submit').removeAttr('disabled');
            }
        }
    });
    BX.addCustomEvent('bfiledselectfiledialogloaded', function(e) {
        $(e.controller).closest('.brf_uploads').addClass('brf_uploads-active');
        $(e.controller).closest('.brf_uploads').collapse();
    });
    BX.addCustomEvent('uploadStart', function(e) {
        $(e.fileInput).closest('.file-selector').addClass('file-selector_hidden');
    });
    $('body').on('click', '.comment__anchor', function(e) {
        e.preventDefault();
        $('._found').removeClass('_found').removeClass('_found_and_faded').removeAttr('data-scrolled');
        let answerContainer = $(e.target).closest('[id^="answercontainer_"');
        answerContainer.addClass('_found');
        let answerHref = new URL(e.target.href);
        let url = new URL(location.href);
        url.searchParams.set('itape', answerHref.searchParams.get('itape'));
        url.hash = '';
        history.replaceState(null, '', url);
        realcommenterScrollToFound();
    });
    BX.addCustomEvent('LHE_ConstructorInited', function(editor) {
        if ($(editor.iFrame).closest('.add-comment__textarea').length) {
            let css = `
                body, .ig-frame-body {
                    border:unset!important;
                    background:transparent!important;
                    color:${document.body.classList.contains('dark-theme') ? '#e4f1f4' : '#2c3450'};
                }
                body:focus, .ig-frame-body:focus {
                    box-shadow:unset!important;
                    border:unset!important;
                }
            `;
            editor.systemCSS += css;
            editor.AppendCSS(css);
            editor.iFrame.contentDocument.body.classList.add('ig-frame-body');
            editor.iFrame.classList.add('lha-iframe_inited', 'form-control');
        }
    });
    $('body').on('click', '.comment__replay', function(e) {
        e.preventDefault();
        if (this.getAttribute('href')) {
            $('.comment__anchor[href="' + this.getAttribute('href') + '"]').click();
        }
    });
