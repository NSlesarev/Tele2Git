    <script>
        $(() => {
            window['ajaxLogin'] = function(submit) {
                let form = submit.closest('#authForm');
                let errText = form.querySelector('.errortext');
                if (submit.hasAttribute('disabled')) return;
                submit.setAttribute('disabled', '');
                errText.style.display = 'none';
                $.ajax({
                    type: 'POST',
                    url: '/local/templates/iguides-2022/ajax/auth.php',
                    data: $(form).serialize(),
                    error: function(req, err) {
                        console.log(req, err);
                        submit.removeAttribute('disabled');
                    },
                    success: function(data) {
                        data = JSON.parse(data);

                        if (data.status) {
                            let url = new URL(location.href);
                            url.search = '?login=yes'
                            location.href = url;
                        } else {
                            errText.innerHTML = data.message;
                            errText.style.display = 'block';
                            submit.removeAttribute('disabled');
                        }
                    }
                });
            };
        });

    </script>

