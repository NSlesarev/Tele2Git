        <script>
            const prefersDarkScheme = window.matchMedia("(prefers-color-scheme: dark)").matches;
            const systemTheme = localStorage.getItem("systemTheme");
            const systemThemeChanged = prefersDarkScheme ? systemTheme == 'light' : systemTheme == 'dark';

            if (systemThemeChanged) {
                localStorage.removeItem('theme');
            }

            const currentTheme = localStorage.getItem("theme");
            if (currentTheme == "dark") {
                document.body.classList.add("dark-theme");
            } else if (currentTheme == "light") {
                document.body.classList.remove("dark-theme");
            }
            if (!currentTheme && prefersDarkScheme) {
                document.body.classList.add("dark-theme");
            }

            localStorage.setItem('systemTheme', prefersDarkScheme ? 'dark' : 'light');
        </script>
